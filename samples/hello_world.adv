target: 'indev'

import: 'hiv/core.adv'
import: 'hiv/list.adv'
import: 'hiv/string.adv'
import: 'hiv/generated/characters.adv'

entrypoint: 'HelloWorld'

class HelloWorld extends Program:
    field output

    method main():
        this.output = Output
        this.output.program = this

        this.output.write(String.+('H').+('e').+('l').+('l').+('o').+(' ').+('w').+('o').+('r').+('l').+('d').+('!').+('\n'))
    end
end
